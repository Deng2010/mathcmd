#include <iostream>
#include <string>
using namespace std;
double dx, d1;
void leftx() {
  int num;
  char type;
  cin >> num >> type;
  if (type == 'x')
    dx += num;
  else
    d1 -= num;
}
void rightx() {
  int num;
  char type;
  cin >> num >> type;
  if (type == 'x')
    dx -= num;
  else
    d1 += num;
}
inline void solvex() {
  string opt;
  while (opt != "exit") {
    printf("mathcmd->solvex> ");
    cin >> opt;
    if (opt == "left") {
      leftx();
    } else if (opt == "right") {
      rightx();
    } else if (opt == "end") {
      cout << d1 / dx << endl;
      dx = d1 = 0;
    }
  }
}
